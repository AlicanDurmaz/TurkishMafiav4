function SquareShake(objectid, d)
{
	if(d == falloutob[0])
	{
		if(Info[shaketimer][d]!=-1)KillTimer(Info[shaketimer][d]),Info[shaketimer][d]=-1;
		return 1;
	}
	switch(Info[shake][d])
	{
		case 0, 5: SetObjectRot(objectid, 31.8, 2, 0);
		case 1, 6: SetObjectRot(objectid, 33.8, 0, 0);
		case 2, 7: SetObjectRot(objectid, 31.8, -2, 0);
		case 3, 8: SetObjectRot(objectid, 29.8, 0, 0);
		case 4, 9: SetObjectRot(objectid, 31.8, 0, 0);
		case 10:
		{
			new Float:X, Float:Y, Float:Z;
			GetObjectPos(objectid, X, Y, Z);
			MoveObject(objectid, X, Y, Z - 100, 40);
		}
		case 11..99: SetObjectRot(objectid, 31.8 - ((Info[shake][d] * 2) - 20), 0, 0);
		case 100:
		{
			DestroyObject(objectid);
   			if(Info[shaketimer][d]!=-1)KillTimer(Info[shaketimer][d]),Info[shaketimer][d]=-1;
		}
	}
	Info[shake][d]++;
	return 1;
}

function DecideWinners()
{
	Info[end] = 1;
	new string[256], winners;
	foreach(new i: Player)
	{
	    if(faloutta{i} == 1)
		{
			winners++;
			format(string, sizeof(string), "~y~~h~[FALLOUT] ~r~~h~%s~w~~h~ falloutu kazandi. Odul: ~r~~h~$%d~w~~h~ ,~r~~h~ +%d~w~~h~ Skor ve ~r~~h~%d~w~~h~ F.P.", PlayerName(i),  1000, 5, 1);
			TextMesaji(TurkceKarakter(string));
			GivePlayerMoney(i, 1000);
			GiveMedal(i, 12);
			PlayerInfo[i][fallkazandi] += 1;
			SetPlayerScore(i, GetPlayerScore(i) + 5);
			GameTextForPlayer(i,"Falloutu kazandin!",5000,3);
			SpawnPlayerEx(i);
		}
	}
    if(Info[timer][2]!=-1)KillTimer(Info[timer][2]),Info[timer][2]=-1;
    falloutbasladi = false;
    falloutkatilimbitti = false;
	if(winners == 0) TextMesaji("~y~~h~[FALLOUT] ~w~~h~Falloutu kazanan yok!");
	return 1;
}

function StartFalling()
{
	if(falloutiptalmi == true)return 1;
	Info[timer][0] = SetTimer("SolarFall", 750, 1);
	Info[timer][2] = SetTimer("LoseGame", 500, 1);
	return 1;
}
function SolarFall()
{
	new objectid, go;
	new alive;
	for(new i = 0; i < 100; i++)
	{
		if(Info[numberout][i] == -1) go++;
	}
	foreach(new a:Player)
	{
		if(faloutta{a} == 1)alive++;
	}
	if(go == 3)
	{
		if(Info[dead] == 0) SetTimer("DecideWinners", 100, 0);
        if(Info[timer][0]!=-1)KillTimer(Info[timer][0]),Info[timer][0]=-1;
		Info[dead] = 1;
		return 1;
	}
	if(alive < 2 && Info[dead] == 0)
	{
		SetTimer("DecideWinners", 100, 0);
		if(Info[timer][0]!=-1)KillTimer(Info[timer][0]),Info[timer][0]=-1;
		Info[dead] = 1;
		return 1;
	}
	start:
	objectid = random(100);
	if(Info[numberout][objectid] != -1)
	{
		#emit JUMP start
	}
	Info[numberout][objectid] = 0;
	Info[shaketimer][objectid] = SetTimerEx("SquareShake", 100, true, "ii", falloutob[objectid],objectid);
	return 1;
}

function CountDown()
{
	if(falltime > GetTickCount())return 1;
	falltime = GetTickCount()+500;
	new string[128], number[8], oyuncu;
	string = "~g~~h~ Basliyor ~y~~h~";
	format(number, sizeof(number), "%d", Info[counts]);
	strins(string, number, strlen(string));
	foreach(new i: Player)
	{
		if(faloutta{i} == 1)
		{
			GameTextForPlayer(i,string, 1000, 3);
			oyuncu++;
		}
	}
	if(Info[counts] <= 0)
	{
		if(Info[timer][1]!=-1)KillTimer(Info[timer][1]),Info[timer][1]=-1;
		falloutkatilimbitti = true;
		if(oyuncu < 2)
		{
			falloutiptal();
			return 1;
		}
		StartFalling();
		return 1;
	}
	SetTimer("CountDown", 1000, 0);
	return Info[counts]--;
}
function falloutiptal()
{
	foreach(new i : Player)
	{
		if(faloutta{i} == 1)SpawnPlayerEx(i);
	}
	TextMesaji("~y~~h~[FALLOUT] ~w~~h~Fallout iptal edildi! (~r~~h~Yetersiz katilim~w~~h~)");
	falloutiptalmi = true;
	falloutkatilimbitti = false;
	falloutbasladi = false;
	return 1;
}
function LoseGame()
{
	new Float:X, Float:Y, Float:Z;
	foreach(new i : Player)
	{
	    if(faloutta{i} == 1)
		{
	    	if(IsPlayerPausedEx(i))SpawnPlayerEx(i);
			GetPlayerPos(i, X, Y, Z);
			if(Z <= 158)
			{
				GameTextForPlayer(i, "~r~Dustun!", 5000, 3);
				SpawnPlayerEx(i);
			}
		}
	}
	return 1;
}
