stock GetPrevDay(day)
{
	new d,m,y,str[32];
	getdate(y,m,d);
	d-=day;
	if(d<= 0)
	{
		m--;
		switch(m)
		{
			case 0:
			{
				m=12;
				d+=31;
				y--;
			}
			case 1,3,5,7,8,10:
			{
				d+=31;
			}
			case 2:
			{
				if(y%4 != 0)
				{
					d+=28;
				}else
				{
					d+=29;
				}
			}
			case 4,6,9,11:
			{
				d+=30;
			}
		}
	}
	format(str,32,"%d.%d.%d",d,m,y);
	return str;
}
