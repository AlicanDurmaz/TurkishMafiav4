    if(Araba[playerid] == 1)  DestroyVehicleEx(ivehicle[playerid]), Araba[playerid] = 0;
    GetPlayerPos(playerid,X,Y,Z);GetPlayerFacingAngle(playerid,Angle);
	ivehicle[playerid] = CreateVehicle(miktar,X,Y,Z,Angle,1,-1,-1);SetVehicleNumberPlate(ivehicle[playerid], "{E23243}Turkish Mafia");
	SetVehicleVirtualWorld(ivehicle[playerid],GetPlayerVirtualWorld(playerid));
	LinkVehicleToInterior(ivehicle[playerid],GetPlayerInterior(playerid));PutPlayerInVehicle(playerid,ivehicle[playerid],0);
	Araba[playerid] = 1;
	format(estring,512,"» Bilgi: {FFFFFF}Kendinize {0077FF}%s({C9404E}%d{0077FF}){FFFFFF} isimli aracý spawn ettiniz.",AracIsimler[miktar-400],miktar);
