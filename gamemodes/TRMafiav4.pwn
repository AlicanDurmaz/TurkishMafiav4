public OnPlayerDeath(playerid, killerid, reason)
{
	if(killerid != INVALID_PLAYER_ID)
	{
	    if(PlayerInfo[playerid][FakeKill] != killerid) return KickReason(playerid,"Fake Kill");
	    if(playerid == killerid) return KickReason(playerid,"Death Hack");
	}
    if(takimda{playerid} == 2)
	{
		takim2sayi --;
	}else
	if(takimda{playerid} == 1)
	{
		takim1sayi --;
	}
    takimda{playerid} = 0;
    savastimer();
	if(shootde{playerid} == 1)
	{
		if(GetPVarInt(playerid,"shootkill") > GetTickCount())
		{
			killerid = GetPVarInt(playerid,"shootvuran");
			format(fstring,256,"~n~~p~~h~~h~YOU WERE KILLED BY~n~~w~~h~%s",PlayerName(killerid));
		    GameTextForPlayer(playerid,fstring,1000,4);
		    RepairVehicle(GetPlayerVehicleID(killerid));
		    format(fstring,256,"~n~~p~~h~~h~YOU KILLED~n~~w~~h~%s",PlayerName(playerid));
		    GameTextForPlayer(killerid,fstring,1000,4);
		}
	}
	if(killerid != INVALID_PLAYER_ID)
