function YarisHesapla()
{
	new
	Float:fx,
    Float:fy,
	Float:fz,
	Float:uzaklik[MAX_PLAYERS];
	foreach(new i:racer)
	{
	    GetPlayerPos(i,fx,fy,fz);
	    fx -= CPCoords[CPProgess[i]][0];
	    fy -= CPCoords[CPProgess[i]][1];
	    fz -= CPCoords[CPProgess[i]][2];
	    uzaklik[i] = ((fx * fx) + (fy * fy) + (fz * fz));
	    RacePosition[i] = 1;
 	}
	foreach(new playerid:racer)
	{
		foreach(new i:racer)
		{
			if(i != playerid)
			{
	    		if(CPProgess[playerid] < CPProgess[i])RacePosition[playerid]++;
	    		else if(CPProgess[playerid] == CPProgess[i])
				{
	    			if(uzaklik[playerid] > uzaklik[i])RacePosition[playerid]++;
	    		}
			}
			RacePosition[playerid] += FinishCount;
			TextInfo(playerid);
		}
	}
	return 1;
}
function PickupCheck()
{
	new
	Float:pos[4],
	Float:Vhiz[3],
	Float:can;
	foreach(new i:racer)
	{
		GetPlayerPos(i,pos[0],pos[1],pos[2]);
    	foreach(new c:nosp)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		    	if(IsPlayerInAnyVehicle(i))
				{
					if(GetTickCount() - tNitro[i] > 3000)
					{
						tNitro[i] = GetTickCount();
		    			AddVehicleComponent(GetPlayerVehicleID(i), 1010);
		    			PlayerPlaySound(i, 1133 ,0, 0, 0);
		    		}
				}
		    }
	    }
   		foreach(new c:rep)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		        if(IsPlayerInAnyVehicle(i))
				{
					if(GetTickCount() - tRepair[i] > 3000)
					{
						tRepair[i] = GetTickCount();
		    			RepairVehicle(GetPlayerVehicleID(i));
		    			PlayerPlaySound(i, 1133 ,0, 0, 0);
		    		}
				}
		    }
	    }
	    foreach(new c:vehp)
	    {
		    if(IsPlayerInRangeOfPoint(i, 3.5, mPickInfo[c][sX], mPickInfo[c][sY], mPickInfo[c][sZz]))
		    {
		        new vehid = GetPlayerVehicleID(i);
		    	if(GetVehicleModel(vehid) != vehchcar[c]){
			    GetVehiclePos(vehid,pos[0],pos[1],pos[2]);
			    GetVehicleZAngle(vehid,pos[3]);
			    GetVehicleVelocity(vehid,Vhiz[0],Vhiz[1],Vhiz[2]);
			    GetVehicleHealth(vehid,can);
			    DestroyVehicleEx(ivehicle[i]);
			    ivehicle[i] = CreateVehicle(vehchcar[c],pos[0],pos[1],pos[2]+1,pos[3],-1,-1,-1);
			    for(new as; as < 5; as++)
				{
					if(VehicleSave[as][i][17] == vehchcar[c])
					{
						for(new a; a < 14;a++)
						{
							if(VehicleSave[as][i][a] != 0)AddVehicleComponent(ivehicle[i], VehicleSave[as][i][a]);
						}
						if(VehicleSave[as][i][14] != 3)ChangeVehiclePaintjob(ivehicle[i], VehicleSave[as][i][14]);
						ChangeVehicleColor(ivehicle[i], VehicleSave[as][i][15], VehicleSave[as][i][16]);
						break;
					}
				}
			    SetPVarInt(i,"RaceVehiclem",vehchcar[c]);
			    SetVehicleVirtualWorld(ivehicle[i],10);
			    PutPlayerInVehicle(i,ivehicle[i],0);
			    SetVehicleVelocity(ivehicle[i],Vhiz[0],Vhiz[1],Vhiz[2]);
			    SetVehicleHealth(ivehicle[i],can);
			    return 1;
	   			}
		    }
	    }
	}
	return 1;
}
dcmd_duello(playerid,params[])
{
    new player1,silahid1,silahid2,skor,tmp1[21],tmp2[21],WeapName2[32];
	if(sscanf(params,"uds[20]s[20]",player1,skor,tmp1,tmp2))return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}/duello [playerid] [skor] [1. silahid/silahismi] [2. silahid/silahismi]");
 	if(!IsPlayerConnected(player1))return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Oyuncu oyunda deðil.");
	if(duelloengelli[player1] == true)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Bu oyuncunun duello isteði kapalýdýr. Sizde kapatmak için {0077FF}/duellokapat {FFFFFF}kullanýn.");
	if(duelloda[player1] == 1)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Bu oyuncu zaten duelloda.");
	if(IsPlayerPausedEx(player1)) return SendClientMessage(playerid, 0x66FFFFFF, "» Bilgi: {FFFFFF}Oyuncu AFK.");
	if(!IsNumeric(tmp1))silahid1 = GetWeaponIDFromName(tmp1); else silahid1 = strval(tmp1);
	if(!IsNumeric(tmp2))silahid2 = GetWeaponIDFromName(tmp2); else silahid2 = strval(tmp2);
	if(!IsValidWeapon(silahid1) || silahid1 == 0 || !IsValidWeapon(silahid2) || silahid2 == 0)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Geçersiz silah id.");
	if(GetPlayerScore(player1) < skor)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Rakibinin Skoru Yetersiz.");
	if(GetPlayerScore(playerid) < skor)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Rakibinin Skoru Yetersiz.");
	if(skor > 50 || skor < 0)return SendClientMessage(playerid,0x66FFFFFF,"» Bilgi: {FFFFFF}Skor 0 - 50 arasýnda olmalýdýr.");
	if(SureYasagi(playerid, "duel", 20)) return 1;
	GetWeaponName(silahid1,WeapName,32);
	GetWeaponName(silahid2,WeapName2,32);
	format(estring,512,"{E23243}%s {FFFFFF}isimli oyuncu sizi duelloya davet etti.\n\nDuello Silahlarý:\n\t{FFFFFF}Silah-1: {E23243}%s\n\t{FFFFFF}Silah-2: {E23243}%s\n{FFFFFF}Duello Bahis skoru = {E23243}%d",PlayerName(playerid),WeapName,WeapName2,skor);
	ShowPlayerDialog(player1,1387,DIALOG_STYLE_MSGBOX,"{E23243}Turkish Mafia - {FFFFFF}Duello Daveti",estring,"Kabul","Red");
	format(estring,512,"» Bilgi: {FFFFFF}%s isimli oyuncuyu duelloya davet ettiniz.",PlayerName(player1));
	SendClientMessage(playerid, 0x66FFFFFF, estring);
	dellodavetci[player1] = playerid;
	dellodavetci[playerid] = player1;
	duelloskor[player1] = skor;
	duelloskor[playerid] = skor;
	duellosilah[player1] = silahid1;
	duellosilah[playerid] = silahid1;
 	duellosilah2[player1] = silahid2;
	duellosilah2[playerid] = silahid2;
	return 1;
}

stock BolgeYukle(bolgid)
{
	new Query[2048];
	format(Query,2048,"SELECT * FROM `bolgeler` WHERE `bolgeid` = '%d'",bolgid);
	new Cache:cache = mysql_query(TRMafia, Query);
	if(cache_num_rows() > 0)
	{
	    if(mysql_fetch_row(Query))
		{
        	sscanf(Query, "e<p<|>ds[25]ddfffs[20]d>", Bolge[bolgid]);
        	bolgezone[bolgid] = GangZoneCreate(Bolge[bolgid][koorx]-45, Bolge[bolgid][koory]-45, Bolge[bolgid][koorx]+45, Bolge[bolgid][koory]+45);
        	if(Bolge[bolgid][bolgeseviye] >= 5) Bolge[bolgid][bolgeseviye] = 5;
        	else if(Bolge[bolgid][bolgeseviye] < 1)Bolge[bolgid][bolgeseviye] = 1;
