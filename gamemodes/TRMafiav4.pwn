function StartRace()
{
	new count=0;
	foreach(new i : Player)
	{
	    if(Joined{i} == true){
	        SetCameraBehindPlayer(i);
	        TogglePlayerControllable(i, true);
	        PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
   			count++;
		}
	}
	yarispickuptimer = SetTimer("PickupCheck",100,true);
	TextDrawSetString(race[2],"GO GO GO!");
	SetTimer("race2text",999,0);
	RaceStarted = 1;
	if(CountTimer!=-1)KillTimer(CountTimer);
	CountTimer=-1;
