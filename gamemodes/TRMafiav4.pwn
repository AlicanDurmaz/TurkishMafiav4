	YTInfo[index][Timer] = SetTimerEx("SarkiBitti", YTInfo[index][Sure] * 1000, false, "d", index);
	return true;
}
stock SureCek(_cSure, &yil, &ay, &gun, &saat, &dakika, &saniye)
{
	yil = ( _cSure / (60 * 60 * 24 * 365) );
    _cSure -= yil * 60 * 60 * 24 * 365;

    ay = ( _cSure / (60 * 60 * 24 * 30) );
    _cSure -= ay * 60 * 60 * 24 * 30;

	gun = ( _cSure / (60 * 60 * 24) );
    _cSure -= gun * 60 * 60 * 24;

	saat = ( _cSure / (60 * 60) );
    _cSure -= saat * 60 * 60;

	dakika = ( _cSure / 60 );
    _cSure -= dakika * 60;

    saniye = _cSure;
}

public CAC_OnCheatDetect(player_id, cheat_id, opt1, opt2)
{
	if(IsValidCheatID(cheat_id) && !g_iCheatStatus[cheat_id]) return 1;

    new cheatName[64];

	if(IsValidCheatID(cheat_id))
	{	strcat(cheatName, g_szCheatNames[cheat_id]);
	}else{
		format(cheatName, sizeof(cheatName), "Bilinmeyen Hile (%i)", cheat_id);
	}
	
	if(IsValidCheatID(cheat_id))
	{
		switch(g_iCheatActions[cheat_id])
		{
			case CAC_ACTION__KICK: KickReason(player_id, cheatName);
			case CAC_ACTION__BAN: BanReason(player_id, cheatName);
			case CAC_ACTION__WARNING:
			{
			    new str[256];
			    format(str, sizeof(str), "» Sampcac: {FFFFFF}%s isimli oyuncu hile kullanýyor olabilir. Hile %s", PlayerName(player_id), cheatName);
				foreach(new i: Player)
				{
					if(PlayerInfo[i][Level] >= 5)
					{
						SendClientMessage(i, 0xEE5555FF, str);
					}
				}
			}
		}
	}
	return 1;
}

getINIString(const filename[], const section[], const item[], result[], max_size = sizeof(result)) {
    new File: inifile;
    new line[512];
    new sectionstr[64], itemstr[64];
    new sectionfound = 0;

    inifile = fopen(filename, io_read);
    if (!inifile) {
        return 0;
    }

    format(sectionstr, sizeof(sectionstr), "[%s]", section);
    format(itemstr, sizeof(itemstr), "%s=", item);

    while (fread(inifile, line)) {
        StripNewLine(line);

        if (line[0] == 0) {
            continue;
        }

        if (!sectionfound) {
            if (!strcmp(line, sectionstr, true, strlen(sectionstr))) {
                sectionfound = 1;
            }
        } else {
            if (line[0] == '[') {
                fclose(inifile);
                return 0;
            }
            if (!strcmp(line, itemstr, true, strlen(itemstr))) {
                format(result, max_size, "%s", line[strlen(itemstr)]);
                fclose(inifile);
                return 1;
            }
        }
    }

    fclose(inifile);
    return 0;
}
IsValidCheatID(cheat_id) return (0 <= cheat_id < CAC_CHEAT__ALL);
