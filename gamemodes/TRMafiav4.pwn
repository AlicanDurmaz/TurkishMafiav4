stock StopSpectate(playerid)
{
    TogglePlayerSpectating(playerid, 0);
    Spectekiler[playerid] = false;
	SetPlayerVirtualWorld(playerid,0);
	PlayerInfo[playerid][SpecID] = INVALID_PLAYER_ID;
	PlayerInfo[playerid][SpecType] = ADMIN_SPEC_TYPE_NONE;
	GameTextForPlayer(playerid,"~n~~n~~n~~w~~h~~h~Spec Modu Kapandi.",1000,3);
	return 1;
}

stock AdvanceSpectate(playerid)
{
	new dondumbasa;
    if(Iter_Count(Player) == 2) { StopSpectate(playerid); return 1; }
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING && PlayerInfo[playerid][SpecID] != INVALID_PLAYER_ID)
	{
	    new x=PlayerInfo[playerid][SpecID]+1;
	    if(x >= MAX_PLAYERS) x = 0;
	    for(; x<=MAX_PLAYERS; x++)
		{
	    	if(x >= MAX_PLAYERS) x = 0,dondumbasa++;
	        if(IsPlayerConnected(x) && x != playerid && (PlayerInfo[playerid][Level] > PlayerInfo[x][Level] && PlayerInfo[playerid][Level] > PlayerInfo[x][Animator] || PlayerInfo[playerid][Animator] > PlayerInfo[x][Animator] && PlayerInfo[playerid][Animator] > PlayerInfo[x][Level]))
			{
				if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpecID] != INVALID_PLAYER_ID || (GetPlayerState(x) != 1 && GetPlayerState(x) != 2 && GetPlayerState(x) != 3))
				{
					continue;
				}else
				{
					StartSpectate(playerid, x);
					break;
				}
			}
			if(dondumbasa > 2)return 1;
		}
	}
	return 1;
}

stock ReverseSpectate(playerid)
{
    new dondumbasa;
    if(Iter_Count(Player) == 2) { StopSpectate(playerid); return 1; }
	if(GetPlayerState(playerid) == PLAYER_STATE_SPECTATING && PlayerInfo[playerid][SpecID] != INVALID_PLAYER_ID)
	{
	    new x = PlayerInfo[playerid][SpecID]-1;
	    if(x < 0)x = MAX_PLAYERS-1;
	    for(; x>=0; x--)
		{
	    	if(x < 0) x = MAX_PLAYERS-1,dondumbasa++;
	        if(IsPlayerConnected(x) && x != playerid && (PlayerInfo[playerid][Level] > PlayerInfo[x][Level] && PlayerInfo[playerid][Level] > PlayerInfo[x][Animator] || PlayerInfo[playerid][Animator] > PlayerInfo[x][Animator] && PlayerInfo[playerid][Animator] > PlayerInfo[x][Level]))
			{
				if(GetPlayerState(x) == PLAYER_STATE_SPECTATING && PlayerInfo[x][SpecID] != INVALID_PLAYER_ID || (GetPlayerState(x) != 1 && GetPlayerState(x) != 2 && GetPlayerState(x) != 3))
				{
					continue;
				}
				else
				{
					StartSpectate(playerid, x);
					break;
				}
			}
			if(dondumbasa > 2)return 1;
		}
	}
	return 1;
}
