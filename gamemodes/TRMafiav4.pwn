dcmd_makeadmin(playerid,params[])
{
	if(PlayerInfo[playerid][LoggedIn] != 1) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Bu komutu kullanmak icin sunucuya giris yapmis olmalisiniz.");
    if(PlayerInfo[playerid][Level] < 10)return YETERSIZSEVIYE(playerid,10);
	new pID, level;
	if(sscanf(params, "id", pID, level)) return SendClientMessage(playerid, 0x66FFFFFF,"» Bilgi: {FFFFFF}/makeadmin [playerid] [level]");
	if(!IsPlayerConnected(pID)) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Oyuncu oyunda deðil.");
	if(PlayerInfo[pID][LoggedIn] != 1) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Oyuncu sunucuya kayitli degil veya giris yapmamis.");
	if(level > 10) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Gecersiz Level Lutfen 1 ile 10 arasinda bir sayi kullaniniz.");
	if(level < 0) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Gecersiz Level Lutfen 1 ile 10 arasinda bir sayi kullaniniz.");
	if(level == PlayerInfo[pID][Level]) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Bu oyuncu zaten admin.");

 	new string[256];
	format(string,sizeof(string),"» Admin: {ffffff}%s isimli yonetici admin seni - %d - level admin yapti.",PlayerName(playerid), level);
	SendClientMessage(pID,0xCC9900FF,string);
	if(level > PlayerInfo[pID][Level])
	{
		GameTextForPlayer(pID, "~b~~h~~h~Admin oldunuz tebrikler", 2000, 3);
	}else {
		GameTextForPlayer(pID, "~b~~h~~h~Admin leveliniz degisti", 2000, 3);
	}
	format(string,sizeof(string),"» Admin: {ffffff}%s isimli oyuncunun levelini %d yaptin.", PlayerName(pID), level);
	SendClientMessage(playerid,0xCC9900FF,string);
	
	PlayerInfo[pID][Level] = level;
	PlayerInfo[pID][Animator] = 0;
	PlayerInfo[pID][DJ] = 0;
	labelguncelle(pID);
	SavePlayer(pID);
	return 1;
}

