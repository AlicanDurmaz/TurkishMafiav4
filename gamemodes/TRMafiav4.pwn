dcmd_anahtarver(playerid, params[])
{
    if(InHouse[playerid] == INVALID_HOUSE_ID) return SendClientMessage(playerid, 0xFF0000FF,"» Hata: {FFFFFF}Herhangi bir evde deðilsiniz.");
    new id, houseid = InHouse[playerid];
    if(HouseData[houseid][Owner] != PlayerInfo[playerid][sqlid]) return SendClientMessage(playerid, 0xFF0000FF,"» Hata: {FFFFFF}Bu evin sahibi deðilsiniz.");
	if(sscanf(params, "u", id)) return SendClientMessage(playerid, 0x66FFFFFF, "» Bilgi: {FFFFFF}/anahtarver [player id]");
	if(id == INVALID_PLAYER_ID) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Oyuncu oyunda deðil!");
	if(id == playerid) return SendClientMessage(playerid, 0xFF0000FF,"» Hata: {FFFFFF}Siz zaten ev sahibisiniz. Anahtara ihtiyacýnýz yok.");
	if(Iter_Contains(HouseKeys[id], houseid)) return SendClientMessage(playerid, 0xFF0000FF,"» Hata: {FFFFFF}Bu oyuncuda zaten anahtar var.");
	Iter_Add(HouseKeys[id], houseid);

	new query[128];
	mysql_format(TRMafia, query, sizeof(query), "INSERT INTO housekeys SET HouseID=%d, Player='%d', Date=UNIX_TIMESTAMP()", houseid, PlayerInfo[id][sqlid]);
	mysql_tquery(TRMafia, query);

	format(query, sizeof(query), "» Bilgi: {FFFFFF}Evinizin anahtarýný %s isimli oyuncuya verdiniz.", PlayerName(id));
	SendClientMessage(playerid, 0x66FFFFFF, query);
	format(query, sizeof(query), "» Bilgi: {FFFFFF}%s isimli oyuncu size evinin anahtarýný verdi. %s", PlayerName(playerid), HouseData[houseid][Name]);
	SendClientMessage(id, 0x66FFFFFF, query);
	return 1;
}

