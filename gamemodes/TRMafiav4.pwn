stock UpdateColor()
{
   if(red == 255 && blue == 0 && green != 255) green += 15;
   if(red != 0 && blue == 0 && green == 255) red -= 15;
   if(red == 0 && blue != 255 && green == 255) blue += 15;
   if(red == 0 && blue == 255 && green != 0) green -= 15;
   if(red != 255 && blue == 255 && green == 0) red += 15;
   if(red == 255 && green == 0 && blue != 0) blue -= 15;
   finalcolorhex = red<<24 | green<<16 | blue<<8 | 255;
}
