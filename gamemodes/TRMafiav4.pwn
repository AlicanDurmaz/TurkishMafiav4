	new miktar,Float:X,Float:Y,Float:Z,Float:Angle;
	if(!IsNumeric(params))
	{
		miktar = AracIsimiGiris(params);
	}
	else
	{
		miktar = strval(params);
	}
	if(miktar < 400 || miktar > 611) return SendClientMessage(playerid,0xFF0000FF,"» Hata: {FFFFFF}Böyle bir araç yok!");
