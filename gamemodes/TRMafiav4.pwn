	cache = mysql_query(TRMafia, Query);
	if(!cache_num_rows())
	{
		mysql_format(TRMafia, Query, sizeof(Query), "INSERT INTO `akas` (`serial`, `name`) VALUES ('%e', '%e')", serial, PlayerName(playerid));
    	mysql_query(TRMafia, Query, false);
	}
	cache_delete(cache);
	mysql_format(TRMafia, Query, sizeof(Query), "SELECT * FROM `akas` WHERE `serial` = '%e'", serial);
	cache = mysql_query(TRMafia, Query);
	new rows = cache_num_rows();
	if(rows > 0)
	{
		new str[2048];
		for(new i; i < rows; i++)
		{
			cache_get_value_name(i, "name", names);
			format(str, sizeof(str), "%s{E23243}%d){E23243}\t%s\n", str, count, names);
			count++;
 		}
        SetPVarString(playerid, "akas", str);
	}
	cache_delete(cache);
}
