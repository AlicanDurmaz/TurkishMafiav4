function derbycontrol()
{
	if(derbybasladi != true)return 1;
	if(derbystarted != true)return 1;
	if(Iter_Count(derbyciler) <= 1)
	{
		derbycount = 0;
		if(derbyno != 1)
		{
			KillTimer(dpickuptimer);
			for(new z;z<2000;z++)DestroyDynamicObject(dObje[z]),dObje[z]=0;
			for(new z;z<500;z++)DestroyDynamic3DTextLabel(labeld[z]),labeld[z]=Text3D:0;
			for(new z;z<500;z++)DestroyDynamicPickup(repidd[z]),repidd[z]=0;
			Iter_Clear(nospd);
			Iter_Clear(repd);
		}
		derbystarted = false;
		derbybasladi = false;
		foreach(new i:derbyciler)
		{
			if(derbyde{i} == 1)
			{
				GivePlayerMoney(i,1000);
				SetPlayerScore(i,GetPlayerScore(i)+5);
				GivePlayerExp(i, 10);
				if(Araba[i] == 1)  DestroyVehicleEx(ivehicle[i]), Araba[i] = 0;
				SpawnPlayerEx(i);
				PlayerInfo[i][derbykazandi]++;
				GiveMedal(i, 9);
				GameTextForPlayer(i,"~r~~h~Derbyi kazandin ~n~~b~~h~5~w~~h~ skor + ~b~~h~1.000$ + ~r~~h~10 exp",5000,3);
				format(estring,sizeof(estring),"~g~~h~[DD] ~r~~h~%s ~w~~h~adli oyuncu derbyi kazandi. Odul ~r~~h~5 ~w~~h~skor + ~r~~h~1.000$  ~w~~h~+ ~r~~h~10 Exp",PlayerName(i));
				TextMesaji(TurkceKarakter(estring));
				break;
			}
		}
		Iter_Clear(derbyciler);
	}
	return 1;
}
