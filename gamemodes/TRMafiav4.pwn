	return 1;
}
function GenelTimer()
{
	UpdateColor();
	TextDrawColor(AltText[1], finalcolorhex);

	parkour_Timer++;
	soru_Timer++;
	mat_Timer++;
	loto_Timer++;
	lotoS_Timer++;
	news_Timer++;
	tuyo_Timer++;
	if(loto_Timer >= 60+60+60)
	{
	    Jackpot = Jackpot + 5;
	    loto_Timer = 0;
	}
	if(lotoS_Timer >= 60+60+60+60+60)
	{
		Draw();
	    lotoS_Timer = 0;
	}
	if(soru_Timer >= 60+60+60+60+30)
	{
	    SoruStart();
	    soru_Timer = 0;
	}
	if(mat_Timer >= 60+60+60+60+60+30)
	{
	    MatematikTesti();
	    mat_Timer = 0;
	}
	if(parkour_Timer >= 60+60+60+60+60+60)
	{
	    ParkourBaslat();
	    parkour_Timer = 0;
	}
	if(tuyo_Timer >= 15)
	{
	    UpdateTuyo();
	    tuyo_Timer = 0;
	}
	if(news_Timer >= 30)
	{
	    news_Timer = 0;
	    if(Iter_Count(Player) > 0) UpdateNews();
	}
	ParkourCheck();
 	foreach(new i: Player)
	{
	    new pingim = GetPlayerPing(i);
		if(pingim >= 1000 && GetPVarInt(i, "SpawnOldumAmk") == 1)
		{
		    KickReason(i,"High Ping");
		    return 1;
		}
        AFK[i]++;
	    if(PlayerInfo[i][textdurum] == true)
	    {
			TextDrawShowForPlayer(i, AltText[1]);
		}
	    FlyKontrol(i);
	    TabloGuncelle(i);
        if(shootde{i} == 1 || derbyde{i} == 1)
        {
        	if(IsPlayerPausedEx(i)) SpawnPlayerEx(i);
		}
		if(!IsPlayerPausedEx(i)) PlayerInfo[i][onlinet]++;
		
		if(PlayerInfo[i][Jailed] == 1)
		{
			if(!IsPlayerPausedEx(i) && PlayerInfo[i][JailTime] > 0)
			{
				PlayerInfo[i][JailTime] -= 1000;
			}else
			if(PlayerInfo[i][JailTime] <= 0)
