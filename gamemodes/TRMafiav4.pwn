function MinigameCountdown()
{
	if(Iter_Count(_Minigamer) < 2)
	{
		TextMesaji("~y~~h~[FALLOUT] ~w~~h~Fallout iptal edildi! (~r~~h~Yetersiz katilim~w~~h~)");
		foreach(new i: _Minigamer) Minigamer_{ i } = false,SpawnPlayerEx(i);
		return EndMinigame();
	}
	if(inProgress != 2)
	{
  		foreach(new i: _Minigamer)
		{
			SilahSil( i );
			SetPlayerInterior( i, 0 );
     		SetPlayerCameraPos( i, -5298.4814,-218.4391,42.1386);
     		SetPlayerCameraLookAt( i, -5298.1616,-189.6903,23.6564);
     		TogglePlayerControllable( i, false );
     		GameTextForPlayer(i,"~g~~h~~h~Oyun Basliyor",2000,3);
		}
		falloutkatilimbitti = true;
		Iter_Clear(_Objects);
		for(new i; i < MAX_SLOTS; i++) Iter_Add(_Objects, i);
		SetTimer("MinigameCountdown", 2000, 0);
		inProgress = 2;
	}
	else
	{
		foreach(new i: _Minigamer)
		{
		    if(!VIEW_FROM_ABOVE)
			SetCameraBehindPlayer( i );
			PlayerPlaySound( i, 1057, 0.0, 0.0, 0.0 );
			TogglePlayerControllable( i, true );
			GameTextForPlayer(i,"~g~~h~~h~Oyun Basladi",2000,3);
		}
		uTimer = SetTimer( "MinigameUpdate", 2500, 1 );
	}
	return 1;
}

function MinigameUpdate()
{
	if( Iter_Count(_Minigamer) < 1 ) return EndMinigame();
	new str[128], Float:playerx, Float:playery, Float:playerz;
	foreach(new i: _Minigamer)
	{
	    if(i > MAX_PLAYERS)continue;
		GetPlayerPos( i, playerx, playery, playerz );
		if(playerz < 2.0)
		{
			format(str, sizeof(str), "~r~~h~%s ~w~~h~dustu!", PlayerName(i));
			foreach(new a: _Minigamer)GameTextForPlayer(a, str ,5000,3);
			Minigamer_{i} = false;
			SpawnPlayerEx(i);
			new
        	next;
			Iter_SafeRemove(_Minigamer, i, next);
			i = next;
		}
	}
	if( 0 < Iter_Count(_Minigamer) < 2)
	{
		MinigameWinner( Iter_First(_Minigamer) );
	}
 	new objectid, Float:ObjectX, Float:ObjectY, Float:ObjectZ;
    if(!VIEW_FROM_ABOVE)
    {
	 	foreach(new i: _Objects)
		{
		    #define IsOdd(%1) ((%1) & 1)
			if(IsOdd(random(10)))
			{
		 	    GetDynamicObjectPos( Objects_[0][i], ObjectX, ObjectY, ObjectZ );
				MoveDynamicObject( Objects_[0][i], ObjectX, ObjectY, ObjectZ -1.5, 0.2 );
	   			MoveDynamicObject( Objects_[1][i], ObjectX, ObjectY, ObjectZ -1.5, 0.2 );
			}
			else
			{
				GetDynamicObjectPos( Objects_[0][i], ObjectX, ObjectY, ObjectZ );
				MoveDynamicObject( Objects_[0][i], ObjectX, ObjectY, ObjectZ +1.5, 0.2 );
				MoveDynamicObject( Objects_[1][i], ObjectX, ObjectY, ObjectZ +1.5, 0.2 );
			}
		}
	}
 	objectid = Iter_Random(_Objects);
	GetDynamicObjectPos( Objects_[0][objectid], ObjectX, ObjectY, ObjectZ );
	SetTimerEx("SpeedUp", 500, 0, "ifff", objectid, ObjectX, ObjectY, ObjectZ);
	MoveDynamicObject( Objects_[0][objectid], ObjectX, ObjectY, ObjectZ -5, 1 );
	if(!VIEW_FROM_ABOVE)
	MoveDynamicObject( Objects_[1][objectid], ObjectX, ObjectY, ObjectZ -5, 1 );
    Iter_Remove(_Objects, objectid );
	return 1;
}

function SpeedUp( object, Float:x, Float:y, Float:z )
{
	MoveDynamicObject( Objects_[0][object], x, y, z -150, 20 );
	if(!VIEW_FROM_ABOVE)
	MoveDynamicObject( Objects_[1][object], x, y, z -150, 20 );
	foreach(new i: _Minigamer) PlayerPlaySound( i, 1039, 0.0, 0.0, 0.0 );
}

function EndMinigame( )
{
	for( new i; i < MAX_SLOTS; i++ )
 	{
 	    DestroyDynamicObject( Objects_[0][i] );
 	    Objects_[0][i] = 0;
 	    if(!VIEW_FROM_ABOVE)
 	    DestroyDynamicObject( Objects_[1][i] ),Objects_[1][i] = 0;
 	}
 	inProgress = 0;
 	falloutbasladi = false;
    falloutkatilimbitti = false;
	Iter_Clear(_Objects);
	Iter_Clear(_Minigamer);
	if(uTimer != -1)KillTimer( uTimer );
	uTimer = -1;
	return 1;
}

function MinigameWinner( player )
{
	if(player == MAX_PLAYERS+1)return 1;
	format(estring, sizeof(estring), "~y~~h~[FALLOUT] ~r~~h~%s~w~~h~ falloutu kazandi. Odul: ~r~~h~$%d~w~~h~ ,~r~~h~ +%d~w~~h~ Skor ve ~r~~h~%d~w~~h~ F.P.", PlayerName(player),  1000, 5, 1);
	TextMesaji(TurkceKarakter(estring));
 	GivePlayerMoney( player, 1000);
 	PlayerInfo[player][fallkazandi] += 1;
	SetPlayerScore(player, GetPlayerScore(player) + 5);
	GameTextForPlayer(player,"Falloutu kazandin!",5000,3);
 	Minigamer_{ player } = false;
 	Iter_Remove(_Minigamer, player);
	SetTimerEx( "OnPlayerSpawn", 1400, 0, "i", player );
	SetTimer( "EndMinigame", 1700, 0);
	return 1;
}
