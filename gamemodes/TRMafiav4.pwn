stock GetWeaponSlot(weaponid)
{
	new slot;
	switch(weaponid)
	{
		case 0, 1: slot = 0;
		case 2..9: slot = 1;
		case 10..15: slot = 10;
		case 16..18, 39: slot = 8;
		case 22..24: slot = 2;
		case 25..27: slot = 3;
		case 28, 29, 32: slot = 4;
		case 30, 31: slot = 5;
		case 33, 34: slot = 6;
		case 35..38: slot = 7;
		case 40: slot = 12;
		case 41..43: slot = 9;
		case 44..46: slot = 11;
	}
	return slot;
}
