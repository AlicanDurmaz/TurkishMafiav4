	MatematikInfo[mIkinciHarf] = randomEx(1, 50);

	switch(random(4))
	{
		case 0: format(MatematikInfo[mIslem], 5, "*");
		case 1: format(MatematikInfo[mIslem], 5, "+");
		case 2: format(MatematikInfo[mIslem], 5, "-");
		case 3: format(MatematikInfo[mIslem], 5, "/");
	}

	MatematikInfo[mSkor] = randomEx(2, 30);
	MatematikInfo[mPara] = randomEx(10, 1000);
	MatematikInfo[mExp] = randomEx(15, 60);

	if(!strcmp(MatematikInfo[mIslem], "+"))
		MatematikInfo[mCevap] = MatematikInfo[mIlkHarf] + MatematikInfo[mIkinciHarf];
	else if(!strcmp(MatematikInfo[mIslem], "-"))
		MatematikInfo[mCevap] = MatematikInfo[mIlkHarf] - MatematikInfo[mIkinciHarf];
	else if(!strcmp(MatematikInfo[mIslem], "*"))
		MatematikInfo[mCevap] = MatematikInfo[mIlkHarf] * MatematikInfo[mIkinciHarf];
	else if(!strcmp(MatematikInfo[mIslem], "/"))
	{
		do
		{
			MatematikInfo[mIlkHarf] = randomEx(1, 300);
			MatematikInfo[mIkinciHarf] = randomEx(1, 40);
			MatematikInfo[mCevap] = MatematikInfo[mIlkHarf] / MatematikInfo[mIkinciHarf];
		}
		while(MatematikInfo[mIlkHarf] % MatematikInfo[mIkinciHarf]);
	}

	format(asilstr, sizeof(asilstr), "» Matematik: {FF3050}%d %s %d {FFFFFF}iþlemini ilk çözen kazanýr!", MatematikInfo[mIlkHarf], MatematikInfo[mIslem], MatematikInfo[mIkinciHarf]);
